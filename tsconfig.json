{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "outDir": "./docs",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "lib": ["ES2015", "DOM"],
      "baseUrl": "./",
      "paths": {
      "*": ["node_modules/*", "src/types/*"]
      },
      "typeRoots": ["./node_modules/@types", "./src/types"]
    },
    "include": ["src/**/*", "data/**/*", "src/types/**/*"],
    "exclude": ["node_modules"]
  }